<h1> Sign up </h1>

<%= form_for @user do |f| %>
	<div class="field">
		<%= f.label :username %><br/>
		<%= f.text_field :username%>
	</div>
	<div class="field">
		Full Name </br>
		<%= f.text_field :prettyname %>
	</div>
	<div class="field">
		<%= f.label :password %></br>
		<%= f.text_field :password %>
	</div>
	<div class="action">
		<%= f.submit "Sign up" %>
	</div>
<% end %>
		